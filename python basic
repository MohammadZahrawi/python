def cars(manufacture,model,**others_features):
    car_details={}
    car_details['manufacture']=manufacture
    car_details['model']=model
    for key, value in others_features.items():
        car_details[key]=value
    return car_details

x=cars('germany','BMW',year=1996,color='red')
print(x)
u=cars('s','a',color='gg')
print(u)




